{
  "name": "wt-hotel-explorer",
  "version": "0.0.0",
  "description": "Web app to show hotel inventory from WT api.",
  "scripts": {
    "lint": "eslint src test --ext jsx,js",
    "test-watch": "jest --watch",
    "test": "rimraf coverage && jest --coverage",
    "coverage": "cat coverage/lcov.info | coveralls",
    "start": "NODE_ENV=development GIT_REV=`git rev-parse --short HEAD` WT_READ_API=https://playground-api.windingtree.com ./node_modules/.bin/webpack-dev-server",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production",
    "build-for-swarm-playground": "NODE_ENV=production GIT_REV=$TRAVIS_COMMIT HASH_ROUTED_BUILD=1 WT_READ_API='https://playground-api.windingtree.com' ./node_modules/.bin/webpack --mode=production",
    "deploy-now-demo": "now --build-env GIT_REV=$TRAVIS_COMMIT --team windingtree --token $NOW_TOKEN --local-config=now-demo.json --docker --public",
    "alias-demo": "now alias --team windingtree --token=$NOW_TOKEN --local-config=now-demo.json",
    "deploy-now-playground": "now --build-env GIT_REV=$TRAVIS_COMMIT --team windingtree --token $NOW_TOKEN --local-config=now-playground.json --docker --public",
    "alias-playground": "now alias --team windingtree --token=$NOW_TOKEN --local-config=now-playground.json",
    "now-start": "serve --single ./public",
    "postalias-playground": "CF_ROOT=$CF_ROOT_PLAYGROUND ./scripts/cloudflare-cache-purge.sh",
    "postalias-demo": "CF_ROOT=$CF_ROOT_DEMO ./scripts/cloudflare-cache-purge.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/windingtree/wt-hotel-explorer"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@windingtree/wt-ui": "^1.0.0",
    "animate.css": "^3.7.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.4.1",
    "currency.js": "^1.1.4",
    "formik": "^1.2.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.3",
    "moment": "^2.22.2",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-leaflet": "^2.0.0",
    "react-loadable": "^5.4.0",
    "react-markdown": "^3.5.0",
    "react-markdown-renderer": "^1.4.0",
    "react-masonry-css": "^1.0.11",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "redux-thunk-actions": "^1.2.1",
    "reselect": "^3.0.1",
    "websocket": "^1.0.26",
    "windingtree-media-web": "git+https://github.com/windingtree/media.git#web-assets"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "react-test-renderer": "^16.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "serve": "^10.0.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-serve": "^2.0.2"
  },
  "jest": {
    "testRegex": "./test/.*.spec.jsx",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.jsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
